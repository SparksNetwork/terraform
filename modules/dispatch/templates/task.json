[
  {
    "name": "dispatch",
    "image": "878160042194.dkr.ecr.us-west-2.amazonaws.com/dispatch:v020ce97",
    "essential": true,
    "memory": 256,
    "cpu": 1024,
    "environment": [
      {
        "name": "FIREBASE_DATABASE_URL",
        "value": "${firebase_database_url}"
      },
      {
        "name": "CREDENTIALS",
        "value": "${credentials}"
      },
      {
        "name": "KINESIS_STREAM",
        "value": "${kinesis_stream}"
      },
      {
        "name": "AWS_REGION",
        "value": "${aws_region}"
      },
      {
        "name": "CLOUDWATCH_LOG_GROUP",
        "value": "dispatch"
      }
    ]
  }
]